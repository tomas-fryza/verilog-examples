    %assign/vec4 v0x104aeb690_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x104aeb690_0;
    %pad/u 32;
    %cmpi/e 11, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x104aeb040_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x104aeb690_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x104aeb040_0, 0;
    %load/vec4 v0x104aeb690_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x104aeb690_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x104ae4420;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x104aeb7d0_0;
    %inv;
    %store/vec4 v0x104aeb7d0_0, 0, 1;
    %jmp T_1;
